  "engines": {
    "node": "8.9.3"
  },
  "scripts": {
    "rimraf": "npx rimraf",
    "cpy": "npx cpy",
    "build": "tsc",
    "clean": "npm run clean:build && npm run clean:tests",
    "copy:assets": "npm run cpy -- CHANGELOG.md dist",
    "conventional-changelog": "conventional-changelog",
    "changelog": "npm run conventional-changelog -- -i CHANGELOG.md -s",
    "changelog:add": "git add --force CHANGELOG.md package.json",
    "changelog:commit": "git commit -m \"Updated CHANGELOG.md\"",
    "test": "test",
    "version": "npx conventional-changelog -i CHANGELOG.md -s -r && npm run build && npm run copy:assets && npm run changelog:add && npm run changelog:commit",
    "postversion": "git push && git push --tags",
    "release": "npx np --contents=dist"
  },
  "devDependencies": {
    "cpy": "^7.3.0",
    "np": "^3.1.0",
    "rimraf": "^3.0.0",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0-beta0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.2"
  },
  "dependencies": {}
}
